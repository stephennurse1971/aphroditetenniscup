{% extends 'base.html.twig' %}

{% block title %}{{ Text1.contentTitle|raw }}{% endblock %}

{% block body %}
    <h3 style="color: red">
        {% if is_granted('ROLE_ADMIN') %}
            <a target="_blank" href="{{ path('cms_copy_edit', {'id': Text1.id}) }}">{{ Text1.name }}</a>
        {% else %}
            {{ Text2.name }}
        {% endif %}
    </h3>
    <!-- Start Css3 Preloader  -->
    <section class="preloader">
        <div class="spinner">
            <div class="bounce1"></div>
            <div class="bounce2"></div>
            <div class="bounce3"></div>
        </div>
    </section>
    <!-- End Css3 Preloader  -->

    <!--Start breadcrumb Section  -->
    <section id="default-breadcrumb">
        <div class="breadcrumb-opacity"></div>
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-md-8 col-sm-8 col-8">
                    <div class="default-breadcrumb-title text-center">
                        <h4>{{ Text1.contentText|raw }}</h4>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--End breadcrumb Section -->


    <!-- My Service section -->
    <section id="my-service-section" class="default-bg-white">
        <!--  Default space  -->
        <div class="default-space"></div>
        <!--  Default space  -->
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="defult-title">
                        <h4>{{ Text1.contentTitle|raw }}</h4>
                    </div>
                </div>
            </div>

            <div class="row service-margin">
                <div class="col-lg-12 col-md-12">
                    <div class="service-item text-center">
                        <span class="service-icon"><i class="icofont icofont-dashboard-web"></i></span>
                        <h4><a href="{{ Text1.hyperlinks }}">{{ Text1.contentTitle|raw }}</a></h4>
                        <p>{{ Text1.contentText|raw }}</p>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12">
                    <div class="service-item text-center">
                        <span class="service-icon"><i class="icofont icofont-web"></i></span>
                        <h4><a href="{{ Text2.hyperlinks }}">{{ Text2.contentTitle|raw }}</a></h4>
                        <p>{{ Text2.contentText|raw }}</p></div>
                </div>
                <div class="col-lg-12 col-md-12">
                    <div class="service-item text-center">
                        <span class="service-icon"><i class="icofont icofont-stock-mobile"></i></span>
                        <h4><a href="{{ Text3.hyperlinks }}">{{ Text3.contentTitle|raw }}</a></h4>
                        <p>{{ Text3.contentText|raw }}</p>
                    </div>
                </div>

            </div>


        </div>
        <!--  Default space  -->
        <div class="default-space"></div>
        <!--  Default space  -->
    </section>
