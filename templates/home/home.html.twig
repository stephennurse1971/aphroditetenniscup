{% extends 'base.html.twig' %}
{% block body %}
    <div class="row">
        {% for photo in photos %}
            {% set image = asset('administration/attachments/websitePhotos')~"/"~photo.photo %}
            {% set fileType =  photo.photo|split('.')|last %}



            <div class="col-lg-3 col-md-4 m-4 p-4">
                <div class="justify-content-center d-flex">
                    <div class="homepage-gallery-image-container">
                        {% if photo.photo is not null %}

                            {% if fileType == 'mp4' or fileType == 'mov' %}
                                <video class="videoByLocationFull" controls="controls">
                                    <source src="{{ image }}" type="video/mp4"/>
                                </video>
                            {% else %}



                                {% if photo.link is not null %}
                                    <a title="{{ photo.title }}" target="_blank" href="{{ photo.link }}">
                                        <img class="homepage-image" src="{{ image }}">
                                    </a>
                                {% else %}
                                    <img title="{{ photo.title }}" class="homepage-image" src="{{ image }}">
                                {% endif %}
                            {% endif %}
                        {% endif %}
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>

