{{ form_start(form) }}

<div class="row">
    <div class="col-md-4">
        <h3><b>Venue details</b></h3>
        {{ form_row(form.venue) }}
        {{ form_row(form.address) }}
        {{ form_row(form.telNumber) }}
        {{ form_row(form.email) }}
        {{ form_row(form.comment) }}
    </div>

    <div class="col-md-6">
        <h3><b>Admin & system</b></h3>
        {{ form_row(form.mapLink) }}
        {{ form_row(form.webLink) }}
        {{ form_row(form.bookingEngine) }}
        {{ form_row(form.towerHamletsId) }}
        {{ form_row(form.londonRegion) }}
    </div>
</div>


<button class="btn btn-outline-success btn-sm mt-2">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
