{% extends 'base.html.twig' %}

{% block title %}Default Tennis Player Availability Hours{% endblock %}

{% block body %}
    <h1 style="color: red">Default Tennis Player Availability Hours</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ default_tennis_player_availability_hour.id }}</td>
            </tr>
            <tr>
                <th>Weekday/Weekend</th>
                <td>{{ default_tennis_player_availability_hour.WeekdayOrWeekend }}</td>
            </tr>
            <tr>
                <th>Hour</th>
                <td>{{ default_tennis_player_availability_hour.hour }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-primary btn-sm" href="{{ path('default_tennis_player_availability_hours_index') }}">Return to list</a>

    <a class="btn btn-outline-danger btn-sm" href="{{ path('default_tennis_player_availability_hours_edit', {'id': default_tennis_player_availability_hour.id}) }}">edit</a>

    {{ include('default_tennis_player_availability_hours/_delete_form.html.twig') }}
{% endblock %}
