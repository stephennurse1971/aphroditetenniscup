<form method="post" action="{{ path('wine_list_delete', {'id': wine_list.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ wine_list.id) }}">
    <button class="btn btn-danger btn-sm">Delete</button>
</form>
